    <TfrxIBXQuery Name="Plano" UserName="Plano" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="Mes" IgnoreDupParams="False" SQL.Text="SELECT A.ID_PLANO,&#13;&#10;       A.ID_CATEG,&#13;&#10;       B.DS_CATEG,&#13;&#10;       A.ID_CCUSTO,&#13;&#10;       C.DS_CCUSTO,&#13;&#10;       A.DS_PLANO,&#13;&#10;       A.VL_VALOR&#13;&#10;  FROM PLANO A&#13;&#10;  LEFT JOIN CATEGORIA B ON B.ID_CATEG = A.ID_CATEG&#13;&#10;  LEFT JOIN CCUSTO C ON C.ID_CCUSTO = A.ID_CCUSTO&#13;&#10; WHERE A.CD_USUARIO = :USUARIO&#13;&#10; ORDER BY C.DS_CCUSTO  " PropData="05704C656674021C0470546F70020C0A506172616D657465727301010645204E616D653D225553554152494F222044617461547970653D226674537472696E67222045787072657373696F6E3D22262336303B43445F5553554152494F262336323B220000"/>
