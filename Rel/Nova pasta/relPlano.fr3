<TfrxReport Version="4.15.10" DotMatrixReport="False" IniFile="D:\projetos\Financ\fontes\Rel\relAnalise.fr3" OldStyleProgress="True" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="39069,9383864931" ReportOptions.Description.Text="" ReportOptions.LastChange="41875,5929007176" ScriptLanguage="PascalScript" ScriptText.Text="var s_categ, s_ccusto : string;&#13;&#10;&#13;&#10;function getMesDescrito(i_mes : integer) : string;&#13;&#10;var meses : array of string;                                                                 &#13;&#10;begin&#13;&#10;   meses := ['Janeiro','Fevereiro','Março','Abril','Maio','Junho','Julho','Agosto','Setembro','Outubro','Novembro','Dezembro'];&#13;&#10;   Result := meses[i_mes-1];                                                                        &#13;&#10;end;          &#13;&#10;&#13;&#10;function getValorRealizado(i_categ : integer; i_ccusto : integer) : double;&#13;&#10;begin&#13;&#10;   Mov.Close;&#13;&#10;   Mov.Sql.Text :=&#13;&#10;      'SELECT COALESCE(-SUM(A.VL_MOVIM),0) VALOR           '+    &#13;&#10;      '  FROM MOVIMENTO A                                  '+    &#13;&#10;      ' WHERE A.DT_MOVIM BETWEEN :D_INI AND :D_FIN         '+&#13;&#10;      '   AND A.CD_USUARIO = :USUARIO                      '+&#13;&#10;      '   AND A.FL_TIPO = ''R''                            '+  &#13;&#10;      '   AND NOT EXISTS(SELECT 1                          '+                                 &#13;&#10;      '                    FROM TRANSFERENCIA C            '+    &#13;&#10;      '                   WHERE (C.ID_MOVDEB = A.ID_MOVIM  '+    &#13;&#10;      '                      OR C.ID_MOVCRE = A.ID_MOVIM)) ';                 &#13;&#10;&#13;&#10;   if i_categ &#62; 0 then&#13;&#10;      Mov.Sql.Add(' AND A.ID_CATEG IN ('+listaCateg(i_categ)+')');&#13;&#10;&#13;&#10;   if i_ccusto &#62; 0 then&#13;&#10;      Mov.Sql.Add(' AND A.ID_CCUSTO IN ('+listaCCusto(i_ccusto)+')');&#13;&#10;&#13;&#10;   if &#60;I_CONTA&#62; &#62; 0 then                                                    &#13;&#10;      Mov.Sql.Add(' AND A.ID_CONTA = '+IntToStr(&#60;I_CONTA&#62;));&#13;&#10;        &#13;&#10;   Mov.ParamByName('D_INI').Value   := &#60;Mes.&#34;D_INI&#34;&#62;;&#13;&#10;   Mov.ParamByName('D_FIN').Value   := &#60;Mes.&#34;D_FIN&#34;&#62;;&#13;&#10;   Mov.ParamByName('USUARIO').Value := &#60;CD_USUARIO&#62;;       &#13;&#10;   Mov.Open;&#13;&#10;     &#13;&#10;   result := Mov.FieldByName('VALOR').Value;       &#13;&#10;end;&#13;&#10;&#13;&#10;function listaCateg(i_cat : integer) : string;&#13;&#10;begin&#13;&#10;   Aux.Close;&#13;&#10;   Aux.Sql.Text :=&#13;&#10;      'WITH RECURSIVE CAT_TREE AS(        '+&#13;&#10;      '   SELECT A.ID_CATEG               '+&#13;&#10;      '     FROM CATEGORIA A              '+&#13;&#10;      '    WHERE A.ID_CATEG = '+IntToStr(i_cat)+                                &#13;&#10;      '    UNION ALL                      '+&#13;&#10;      '                                   '+&#13;&#10;      '   SELECT A.ID_CATEG               '+&#13;&#10;      '     FROM CATEGORIA A              '+&#13;&#10;      '    INNER JOIN CAT_TREE B ON       '+&#13;&#10;      '          B.ID_CATEG = A.ID_CATPAI)'+&#13;&#10;      '                                   '+&#13;&#10;      '   SELECT LIST(A.ID_CATEG) LISTA   '+&#13;&#10;      '     FROM CAT_TREE A               ';&#13;&#10;   Aux.Open;&#13;&#10;&#13;&#10;   Result := Aux.FieldByName('LISTA').Value;  &#13;&#10;end;&#13;&#10;&#13;&#10;function listaCCusto(i_cc : integer) : string;  &#13;&#10;begin&#13;&#10;   Aux.Close;&#13;&#10;   Aux.Sql.Text :=    &#13;&#10;    'WITH RECURSIVE CC_TREE AS(         '+&#13;&#10;    '   SELECT A.ID_CCUSTO              '+&#13;&#10;    '     FROM CCUSTO A                 '+&#13;&#10;    '    WHERE A.ID_CCUSTO = '+IntToStr(i_cc)+                                             &#13;&#10;    '    UNION ALL                      '+&#13;&#10;    '                                   '+&#13;&#10;    '   SELECT A.ID_CCUSTO              '+&#13;&#10;    '     FROM CCUSTO A                 '+&#13;&#10;    '    INNER JOIN CC_TREE B ON        '+&#13;&#10;    '          B.ID_CCUSTO = A.ID_CCPAI)'+&#13;&#10;    '                                   '+&#13;&#10;    '   SELECT LIST(A.ID_CCUSTO) LISTA  '+&#13;&#10;    '     FROM CC_TREE A                ';&#13;&#10;   Aux.Open;&#13;&#10;&#13;&#10;   Result := Aux.FieldByName('LISTA').Value;        &#13;&#10;end;          &#13;&#10;&#13;&#10;procedure ddPlanoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     if &#60;Plano.&#34;ID_CATEG&#34;&#62; &#62; 0 then                                               &#13;&#10;      s_categ := IntToStr(&#60;Plano.&#34;ID_CATEG&#34;&#62;)+' - '+&#60;Plano.&#34;DS_CATEG&#34;&#62;&#13;&#10;   else&#13;&#10;      s_categ := '&#60;Não Definido&#62;';&#13;&#10;        &#13;&#10;   if &#60;Plano.&#34;ID_CCUSTO&#34;&#62; &#62; 0 then                                               &#13;&#10;      s_ccusto := IntToStr(&#60;Plano.&#34;ID_CCUSTO&#34;&#62;)+' - '+&#60;Plano.&#34;DS_CCUSTO&#34;&#62;&#13;&#10;   else&#13;&#10;      s_ccusto := '&#60;Não Definido&#62;';  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure procSelectPlano;&#13;&#10;begin&#13;&#10;   Plano.Close;&#13;&#10;   Plano.Sql.Text :=       &#13;&#10;      'SELECT A.ID_PLANO,                                '+    &#13;&#10;      '       A.ID_CATEG,                                '+    &#13;&#10;      '       B.DS_CATEG,                                '+    &#13;&#10;      '       A.ID_CCUSTO,                               '+    &#13;&#10;      '       C.DS_CCUSTO,                               '+    &#13;&#10;      '       A.DS_PLANO,                                '+    &#13;&#10;      '       A.VL_VALOR                                 '+    &#13;&#10;      '  FROM PLANO A                                    '+    &#13;&#10;      '  LEFT JOIN CATEGORIA B ON B.ID_CATEG = A.ID_CATEG'+    &#13;&#10;      '  LEFT JOIN CCUSTO C ON C.ID_CCUSTO = A.ID_CCUSTO '+    &#13;&#10;      ' WHERE A.CD_USUARIO = :USUARIO                    ';&#13;&#10;        &#13;&#10;   if &#60;I_CATEG&#62; &#62; 0 then                                                  &#13;&#10;      Plano.Sql.Add(' AND A.ID_CATEG IN ('+listaCateg(&#60;I_CATEG&#62;)+') ');&#13;&#10;&#13;&#10;   if &#60;I_CCUSTO&#62; &#62; 0 then                                                  &#13;&#10;      Plano.Sql.Add(' AND A.ID_CCUSTO IN ('+listaCCusto(&#60;I_CCUSTO&#62;)+') ');&#13;&#10;&#13;&#10;  Plano.Sql.Add(' ORDER BY DS_CATEG ');      &#13;&#10;end;          &#13;&#10;&#13;&#10;procedure mdMesOnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  procSelectPlano;  &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;                                                                  &#13;&#10;end." PropData="">
