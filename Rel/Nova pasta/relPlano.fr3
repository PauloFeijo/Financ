    <TfrxIBXQuery Name="Mes" UserName="Mes" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT DISTINCT EXTRACT(MONTH FROM A.DT_MOVIM) MES,&#13;&#10;       EXTRACT(YEAR FROM A.DT_MOVIM) ANO,&#13;&#10;       CAST(EXTRACT(MONTH FROM A.DT_MOVIM)||'/'||'01'||'/'||EXTRACT(YEAR FROM A.DT_MOVIM) AS DATE) D_INI,&#13;&#10;       DATEADD(1 MONTH TO CAST(EXTRACT(MONTH FROM A.DT_MOVIM)||'/'||'01'||'/'||EXTRACT(YEAR FROM A.DT_MOVIM) AS DATE)) -1 D_FIN&#13;&#10;  FROM MOVIMENTO A&#13;&#10; WHERE A.DT_MOVIM BETWEEN :D_INI AND :D_FIN  &#13;&#10;   AND A.CD_USUARIO = :USUARIO             &#13;&#10;   AND A.FL_TIPO = 'R'&#13;&#10; ORDER BY D_INI       " PropData=""/>
